              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "uid": {
                      "type": "number",
                      "description": "Message UID"
                    },
                    "seq": {
                      "type": "number",
                      "description": "Message sequence number"
                    },
                    "envelope": {
                      "type": "object",
                      "description": "Email envelope information",
                      "properties": {
                        "subject": {
                          "type": "string",
                          "description": "Email subject"
                        },
                        "from": {
                          "type": "array",
                          "description": "From addresses"
                        },
                        "to": {
                          "type": "array",
                          "description": "To addresses"
                        },
                        "date": {
                          "type": "string",
                          "format": "date-time",
                          "description": "Email date"
                        }
                      }
                    },
                    "contentText": {
                      "type": "string",
                      "description": "Plain text content of the email"
                    },
                    "contentHtml": {
                      "type": "string",
                      "description": "HTML content of the email"
                    },
                    "attachments": {
                      "type": "array",
                      "description": "Email attachments",
                      "items": {
                        "type": "object",
                        "properties": {
                          "meta": {
                            "type": "object",
                            "description": "Attachment metadata",
                            "properties": {
                              "filename": {
                                "type": "string",
                                "description": "Attachment filename"
                              },
                              "contentType": {
                                "type": "string",
                                "description": "MIME content type"
                              },
                              "size": {
                                "type": "number",
                                "description": "Size in bytes"
                              }
                            }
                          },
                          "content": {
                            "type": "string",
                            "description": "Base64-encoded attachment content"
                          }
                        }
                      }
                    }
                  }
                    "uid": 123,
                  "seq": 1,
                  "envelope": {
                    "subject": "Test Email",
                    "from": [
                      {
                        "address": "sender@example.com",
                        "name": "Sender"
                      }
                    ],
                    "to": [
                      {
                        "address": "recipient@example.com",
                        "name": "Recipient"
                      }
                    ],
                    "date": "2023-01-15T10:30:00Z"
                  },
                  "contentText": "Hello, this is a test email.",
                  "contentHtml": "<p>Hello, this is a test email.</p>",
                  "attachments": []
