{
  "openapi": "3.0.0",
  "info": {
    "title": "@digipair/skill-exif",
    "summary": "EXIF extraction",
    "description": "This skill allows parsing and extracting EXIF metadata from images.",
    "version": "0.1.0",
    "x-icon": "üñºÔ∏è"
  },
  "paths": {
    "/parse": {
      "post": {
        "tags": ["service"],
        "summary": "Parse",
        "description": "Parses EXIF data from the provided image content.",
        "parameters": [
          {
            "name": "content",
            "summary": "Content",
            "required": true,
            "description": "Image content as a base64-encoded string or binary string",
            "schema": {
              "type": "string"
            }
          }
        ],
        "x-events": [],
        "responses": {
          "200": {
            "description": "EXIF metadata successfully parsed",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "tags": {
                      "type": "object",
                      "description": "EXIF tags containing camera settings, date/time, GPS coordinates, etc.",
                      "additionalProperties": true
                    },
                    "imageSize": {
                      "type": "object",
                      "description": "Image dimensions",
                      "properties": {
                        "width": {
                          "type": "number",
                          "description": "Image width in pixels"
                        },
                        "height": {
                          "type": "number",
                          "description": "Image height in pixels"
                        }
                      }
                    },
                    "thumbnailOffset": {
                      "type": "number",
                      "description": "Offset to thumbnail data in bytes"
                    },
                    "thumbnailLength": {
                      "type": "number",
                      "description": "Length of thumbnail data in bytes"
                    },
                    "thumbnailType": {
                      "type": "number",
                      "description": "Thumbnail image type"
                    },
                    "app1Offset": {
                      "type": "number",
                      "description": "Offset to APP1 segment"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {}
  },
  "x-scene-blocks": {}
}