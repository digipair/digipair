{
  "openapi": "3.0.0",
  "info": {
    "title": "@digipair/skill-yaml",
    "summary": "YAML data management",
    "description": "This skill allows manipulating YAML data",
    "version": "0.1.0",
    "x-icon": "üìù"
  },
  "paths": {
    "/load": {
      "post": {
        "tags": ["service"],
        "summary": "Load YAML",
        "parameters": [
          {
            "name": "yaml",
            "summary": "Data",
            "required": true,
            "description": "YAML data to load",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "options",
            "summary": "Options",
            "required": false,
            "description": "Loading options",
            "schema": {
              "type": "object"
            }
          }
        ],
        "x-events": [],
        "responses": {
          "200": {
            "description": "YAML data successfully parsed to JavaScript object/array",
            "content": {
              "application/json": {
                "schema": {
                  "oneOf": [
                    {
                      "type": "object",
                      "description": "Parsed YAML object",
                      "additionalProperties": true
                    },
                    {
                      "type": "array",
                      "description": "Parsed YAML array",
                      "items": {}
                    },
                    {
                      "type": "string",
                      "description": "Parsed YAML primitive value"
                    },
                    {
                      "type": "number",
                      "description": "Parsed YAML number"
                    },
                    {
                      "type": "boolean",
                      "description": "Parsed YAML boolean"
                    },
                    {
                      "type": "null",
                      "description": "Parsed YAML null value"
                    }
                  ]
                }
              }
            }
          }
        }
      }
    },
    "/dump": {
      "post": {
        "tags": ["service"],
        "summary": "Convert to YAML",
        "parameters": [
          {
            "name": "data",
            "summary": "Data",
            "required": true,
            "description": "Data to convert to YAML",
            "schema": {}
          },
          {
            "name": "options",
            "summary": "Options",
            "required": false,
            "description": "Conversion options",
            "schema": {
              "type": "object"
            }
          }
        ],
        "x-events": [],
        "responses": {
          "200": {
            "description": "Data successfully converted to YAML format",
            "content": {
              "text/yaml": {
                "schema": {
                  "type": "string",
                  "description": "YAML formatted string representation of the input data"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {}
