    "styleHtml": "color-scheme:light;\npadding: 60px 40px 80px 40px;",
    "browserInitialize": [
      {
        "library": "@digipair/skill-web-interact",
        "element": "goTo",
        "properties": {
          "url": "EVALUATE:\"/studio/consumptions-monthly/\" + substring(string(now()), 1, 7)"
        },
        "conditions": { "if": "EVALUATE:count(request.params) = 0" }
      },
      {
        "library": "@digipair/skill-basic",
        "element": "setVariable",
        "properties": { "name": "date", "value": "{{request.params.[0]}}" }
      },
      {
        "library": "@digipair/skill-basic",
        "element": "setVariable",
        "properties": {
          "name": "consumptions",
          "execute": [
            {
              "library": "@digipair/skill-web",
              "element": "executeFactory",
              "properties": {
                "execute": [
                  {
                    "library": "@digipair/skill-logger",
                    "element": "read",
                    "properties": {
                      "date": "EVALUATE:variables.date",
                      "type": "consumption-monthly"
                    }
                  },
                  {
                    "library": "@digipair/skill-basic",
                    "element": "transform",
                    "properties": { "value": "EVALUATE:reverse(previous)" }
                  }
                ]
              }
            }
          ]
        }
      },
      {
        "library": "@digipair/skill-basic",
        "element": "setVariable",
        "properties": {
          "name": "dates",
          "execute": [
            {
              "library": "@digipair/skill-web",
              "element": "executeFactory",
              "properties": {
                "execute": [
                  {
                    "library": "@digipair/skill-logger",
                    "element": "list",
                    "properties": {
                      "type": "consumption-monthly"
                    }
                  }
                ]
              }
            }
          ]
        }
