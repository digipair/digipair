{
  "description": "Editeur de raisonnement",
  "summary": "Editeur de raisonnement",
  "library": "@digipair/skill-web",
  "element": "page",
  "metadata": {},
  "properties": {
    "body": [
      {
        "library": "@digipair/skill-web-spectrum",
        "element": "sp-theme",
        "pins": [
          {
            "library": "web",
            "element": "section",
            "pins": [
              {
                "library": "@digipair/skill-web-spectrum",
                "element": "sp-top-nav",
                "pins": [
                  {
                    "library": "@digipair/skill-web-spectrum",
                    "element": "sp-top-nav-item",
                    "pins": [
                      {
                        "library": "web",
                        "element": "img",
                        "properties": {
                          "src": "https://res.cloudinary.com/do87nxq3l/image/upload/fl_preserve_transparency/v1717769492/logo-digipair_oyvvxz.png?_s=public-apps",
                          "style": "height: 20px;"
                        }
                      }
                    ],
                    "properties": {
                      "href": "https://www.digipair.ai"
                    }
                  },
                  {
                    "library": "@digipair/skill-web-spectrum",
                    "element": "sp-top-nav-item",
                    "properties": {
                      "textContent": "Tool agents",
                      "href": "/studio/tools"
                    }
                  },
                  {
                    "library": "@digipair/skill-web-spectrum",
                    "element": "sp-top-nav-item",
                    "properties": {
                      "textContent": "Worker agents",
                      "href": "/studio/workers"
                    }
                  },
                  {
                    "library": "@digipair/skill-web-spectrum",
                    "element": "sp-action-group",
                    "pins": [
                      {
                        "library": "@digipair/skill-web-spectrum",
                        "element": "sp-action-button",
                        "pins": [
                          {
                            "library": "@digipair/skill-web-font-awesome",
                            "element": "digipair-font-awesome-icon",
                            "properties": {
                              "name": "backward-step",
                              "category": "solid"
                            }
                          }
                        ],
                        "properties": {
                          "dir": "rtl",
                          "textContent": "{{variables.digipair.name}}",
                          "quiet": true
                        },
                        "events": {
                          "click": [
                            {
                              "library": "@digipair/skill-web-interact",
                              "element": "goTo",
                              "properties": {
                                "url": "/studio/tool-tasks/{{request.params.[0]}}"
                              }
                            }
                          ]
                        }
                      },
                      {
                        "library": "@digipair/skill-web-spectrum",
                        "element": "sp-action-button",
                        "pins": [
                          {
                            "library": "@digipair/skill-web-font-awesome",
                            "element": "digipair-font-awesome-icon",
                            "properties": {
                              "name": "file-code",
                              "category": "regular"
                            }
                          }
                        ],
                        "properties": {
                          "quiet": true
                        },
                        "events": {
                          "click": [
                            {
                              "library": "@digipair/skill-web-interact",
                              "element": "goTo",
                              "properties": {
                                "url": "/studio/tool-task-json/{{variables.digipair.id}}/{{variables.reasoning.id}}"
                              }
                            }
                          ]
                        }
                      },
                      {
                        "library": "@digipair/skill-web-spectrum",
                        "element": "sp-action-button",
                        "pins": [
                          {
                            "library": "@digipair/skill-web-font-awesome",
                            "element": "digipair-font-awesome-icon",
                            "properties": {
                              "name": "eye",
                              "category": "regular"
                            }
                          }
                        ],
                        "properties": {
                          "quiet": true
                        },
                        "events": {
                          "click": [
                            {
                              "library": "@digipair/skill-web-interact",
                              "element": "goTo",
                              "properties": {
                                "url": "/{{variables.digipair.id}}/{{variables.reasoning.id}}",
                                "target": "_blank"
                              }
                            }
                          ]
                        },
                        "conditions": {
                          "if": "FEEL:variables.reasoning.element = \"page\""
                        }
                      },
                      {
                        "library": "@digipair/skill-web-spectrum",
                        "element": "sp-action-button",
                        "pins": [
                          {
                            "library": "@digipair/skill-web-font-awesome",
                            "element": "digipair-font-awesome-icon",
                            "properties": {
                              "name": "floppy-disk",
                              "category": "regular"
                            }
                          }
                        ],
                        "properties": {
                          "quiet": true
                        },
                        "events": {
                          "click": [
                            {
                              "library": "@digipair/skill-web-interact",
                              "element": "getAttribute",
                              "properties": {
                                "selector": "digipair-editor",
                                "attribute": "codeInWorkspace"
                              }
                            },
                            {
                              "library": "@digipair/skill-web",
                              "element": "executeFactory",
                              "properties": {
                                "execute": [
                                  {
                                    "library": "@digipair/skill-editor",
                                    "element": "setReasoning",
                                    "properties": {
                                      "digipair": "{{request.params.[0]}}",
                                      "reasoning": "{{request.params.[1]}}",
                                      "value": "FEEL:parent.previous"
                                    }
                                  }
                                ]
                              }
                            },
                            {
                              "library": "@digipair/skill-web-notification",
                              "element": "information",
                              "properties": {
                                "message": "Enregistrement effectu√© !"
                              }
                            }
                          ]
                        }
                      }
                    ],
                    "properties": {
                      "style": "margin-inline-start: auto;",
                      "size": "s"
                    }
                  }
                ]
              }
            ],
            "properties": {
              "style": "background-color: white;\npadding: 0 20px 0 20px;\nposition: fixed;\nz-index: 10000;\ntop: 0;\nleft: 0;\nright: 0;"
            }
          },
          {
            "library": "@digipair/skill-web-editor",
            "element": "digipair-editor",
            "properties": {
              "reasoning": "FEEL:variables.reasoning",
              "schemas": "FEEL:variables.schemas",
              "menuBackgroundColor": "#212e3c",
              "contentStyle": "position: fixed; top: 47px; right: 0; bottom: 0; left: 0;",
              "language": "{{variables.lang}}"
            }
          }
        ],
        "properties": {
          "color": "light",
          "scale": "medium"
        }
      },
      {
        "library": "@digipair/skill-web-chatbot",
        "element": "digipair-chatbot",
        "properties": {
          "code": "{{request.digipair}}"
        }
      }
    ],
    "styleHtml": "color-scheme:light; \nbackground-color: #f4f4f4;",
    "browserInitialize": [
      {
        "library": "@digipair/skill-basic",
        "element": "setVariable",
        "properties": {
          "name": "digipair",
          "execute": [
            {
              "library": "@digipair/skill-web",
              "element": "executeFactory",
              "properties": {
                "execute": [
                  {
                    "library": "@digipair/skill-editor",
                    "element": "digipair",
                    "properties": {
                      "digipair": "{{request.params.[0]}}"
                    }
                  }
                ]
              }
            }
          ]
        }
      },
      {
        "library": "@digipair/skill-basic",
        "element": "setVariable",
        "properties": {
          "name": "reasoning",
          "execute": [
            {
              "library": "@digipair/skill-web",
              "element": "executeFactory",
              "properties": {
                "execute": [
                  {
                    "library": "@digipair/skill-editor",
                    "element": "reasoning",
                    "properties": {
                      "digipair": "{{request.params.[0]}}",
                      "reasoning": "{{request.params.[1]}}"
                    }
                  }
                ]
              }
            }
          ]
        }
      },
      {
        "library": "@digipair/skill-basic",
        "element": "setVariable",
        "properties": {
          "name": "lang",
          "value": "en"
        }
      },
      {
        "library": "@digipair/skill-basic",
        "element": "setVariable",
        "properties": {
          "name": "schemas",
          "execute": [
            {
              "library": "@digipair/skill-web",
              "element": "executeFactory",
              "properties": {
                "execute": [
                  {
                    "library": "@digipair/skill-basic",
                    "element": "setVariable",
                    "properties": {
                      "name": "schemasLibraries",
                      "execute": [
                        {
                          "library": "@digipair/skill-editor",
                          "element": "schemas",
                          "properties": {
                            "libraries": "FEEL:context(get entries(variables.digipair.libraries)[substring(item.key, 1, 9) != \"digipair:\"])",
                            "language": "en"
                          }
                        }
                      ]
                    }
                  },
                  {
                    "library": "@digipair/skill-basic",
                    "element": "setVariable",
                    "properties": {
                      "name": "schemasDigipair",
                      "execute": [
                        {
                          "library": "@digipair/skill-factory",
                          "element": "start",
                          "properties": {
                            "reasoning": "schema.json",
                            "digipair": "{{item}}"
                          },
                          "conditions": {
                            "each": "FEEL: concatenate(\n  [request.params[1]], \n  for digipair in \n    get entries(variables.digipair.libraries)[starts with(key, \"digipair:\") and not(starts with(value, \"http\"))]\n    return substring(digipair.key, 10)\n)"
                          }
                        }
                      ]
                    }
                  },
                  {
                    "library": "@digipair/skill-basic",
                    "element": "setVariable",
                    "properties": {
                      "name": "schemasRemote",
                      "execute": [
                        {
                          "library": "@digipair/skill-basic",
                          "element": "transform",
                          "properties": {
                            "execute": [
                              {
                                "library": "@digipair/skill-http",
                                "element": "read",
                                "properties": {
                                  "path": "FEEL:item.value + \"/\" + split(item.key, \"digipair:\")[2] + \"/schema.json\""
                                }
                              }
                            ]
                          },
                          "conditions": {
                            "each": "FEEL:get entries(variables.digipair.libraries)[starts with(key, \"digipair:\") and starts with(value, \"http\")]"
                          }
                        }
                      ]
                    }
                  },
                  {
                    "library": "@digipair/skill-basic",
                    "element": "transform",
                    "properties": {
                      "value": "FEEL:flatten([variables.schemasLibraries, variables.schemasRemote, variables.schemasDigipair])"
                    }
                  }
                ]
              }
            }
          ]
        }
      }
    ],
    "confirmBeforeUnload": "true"
  },
  "pins": []
}