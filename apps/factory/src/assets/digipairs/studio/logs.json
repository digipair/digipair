{
  "description": "Liste des logs",
  "summary": "Logs",
  "library": "@digipair/skill-web",
  "element": "page",
  "metadata": {},
  "properties": {
    "ssr": false,
    "body": [
      {
        "library": "@digipair/skill-web-spectrum",
        "element": "sp-theme",
        "pins": [
          {
            "library": "web",
            "element": "section",
            "pins": [
              {
                "library": "@digipair/skill-web-spectrum",
                "element": "sp-top-nav",
                "pins": [
                  {
                    "library": "@digipair/skill-web-spectrum",
                    "element": "sp-top-nav-item",
                    "pins": [
                      {
                        "library": "web",
                        "element": "img",
                        "properties": {
                          "src": "https://res.cloudinary.com/do87nxq3l/image/upload/fl_preserve_transparency/v1717769492/logo-digipair_oyvvxz.png?_s=public-apps",
                          "style": "height: 20px;"
                        }
                      }
                    ],
                    "properties": {
                      "href": "https://www.digipair.ai"
                    }
                  },
                  {
                    "library": "@digipair/skill-web-spectrum",
                    "element": "sp-top-nav-item",
                    "properties": {
                      "textContent": "Tool agents",
                      "href": "/studio/tools"
                    }
                  },
                  {
                    "library": "@digipair/skill-web-spectrum",
                    "element": "sp-top-nav-item",
                    "properties": {
                      "textContent": "Worker agents",
                      "href": "/studio/workers"
                    }
                  },
                  {
                    "library": "@digipair/skill-web-spectrum",
                    "element": "sp-top-nav-item",
                    "properties": {
                      "textContent": "Conversationnal agents",
                      "href": "/studio/pairs"
                    }
                  },
                  {
                    "library": "@digipair/skill-web-spectrum",
                    "element": "sp-action-menu",
                    "pins": [
                      {
                        "library": "@digipair/skill-web-spectrum",
                        "element": "sp-menu-item",
                        "properties": {
                          "textContent": "View logs",
                          "href": "/studio/logs"
                        }
                      },
                      {
                        "library": "@digipair/skill-web-spectrum",
                        "element": "sp-menu-item",
                        "properties": {
                          "textContent": "View processes",
                          "href": "/studio/process-list"
                        }
                      },
                      {
                        "library": "@digipair/skill-web-spectrum",
                        "element": "sp-menu-item",
                        "properties": {
                          "textContent": "View LLM consumption",
                          "href": "/studio/consumptions"
                        }
                      },
                      {
                        "library": "@digipair/skill-web-spectrum",
                        "element": "sp-menu-item",
                        "properties": {
                          "textContent": "View the monthly LLM consumption",
                          "href": "/studio/consumptions-monthly"
                        }
                      },
                      {
                        "library": "@digipair/skill-web-spectrum",
                        "element": "sp-menu-item",
                        "properties": {
                          "textContent": "Settings",
                          "href": "/studio/common-edit"
                        }
                      }
                    ],
                    "properties": {
                      "style": "margin-inline-start: auto;",
                      "quiet": true
                    }
                  }
                ]
              }
            ],
            "properties": {
              "style": "background-color: white;\npadding: 0 20px 0 20px;\nposition: fixed;\nz-index: 10000;\ntop: 0;\nleft: 0;\nright: 0;"
            }
          },
          {
            "library": "web",
            "element": "section",
            "properties": {
              "style": "margin: 40px 0 20px 0;",
              "textContent": "Date"
            }
          },
          {
            "library": "@digipair/skill-web-spectrum",
            "element": "sp-action-group",
            "pins": [
              {
                "library": "@digipair/skill-web-spectrum",
                "element": "sp-action-button",
                "properties": {
                  "textContent": "{{item}}",
                  "disabled": "EVALUATE:item = variables.date"
                },
                "events": {
                  "click": [
                    {
                      "library": "@digipair/skill-web-interact",
                      "element": "goTo",
                      "properties": {
                        "url": "/studio/logs/{{item}}"
                      }
                    }
                  ]
                },
                "conditions": {
                  "each": "EVALUATE:variables.dates"
                }
              }
            ]
          },
          {
            "library": "web",
            "element": "section",
            "properties": {
              "style": "margin: 40px 0 20px 0;",
              "textContent": "Liste des logs"
            }
          },
          {
            "library": "@digipair/skill-web-spectrum",
            "element": "sp-table",
            "pins": [
              {
                "library": "@digipair/skill-web-spectrum",
                "element": "sp-table-head",
                "pins": [
                  {
                    "library": "@digipair/skill-web-spectrum",
                    "element": "sp-table-head-cell",
                    "properties": {
                      "textContent": "Heure"
                    }
                  },
                  {
                    "library": "@digipair/skill-web-spectrum",
                    "element": "sp-table-head-cell",
                    "properties": {
                      "textContent": "Digipair"
                    }
                  },
                  {
                    "library": "@digipair/skill-web-spectrum",
                    "element": "sp-table-head-cell",
                    "properties": {
                      "textContent": "Raisonnement"
                    }
                  },
                  {
                    "library": "@digipair/skill-web-spectrum",
                    "element": "sp-table-head-cell",
                    "properties": {
                      "textContent": "Type"
                    }
                  },
                  {
                    "library": "@digipair/skill-web-spectrum",
                    "element": "sp-table-head-cell",
                    "properties": {
                      "textContent": "Message"
                    }
                  }
                ]
              },
              {
                "library": "@digipair/skill-web-spectrum",
                "element": "sp-table-body",
                "pins": [
                  {
                    "library": "@digipair/skill-web-spectrum",
                    "element": "sp-table-row",
                    "pins": [
                      {
                        "library": "@digipair/skill-web-spectrum",
                        "element": "sp-table-cell",
                        "properties": {
                          "textContent": "EVALUATE:substring(fromTime(item.date), 12, 8)"
                        }
                      },
                      {
                        "library": "@digipair/skill-web-spectrum",
                        "element": "sp-table-cell",
                        "properties": {
                          "textContent": "{{item.digipair}}"
                        }
                      },
                      {
                        "library": "@digipair/skill-web-spectrum",
                        "element": "sp-table-cell",
                        "properties": {
                          "textContent": "{{item.reasoning}}"
                        }
                      },
                      {
                        "library": "@digipair/skill-web-spectrum",
                        "element": "sp-table-cell",
                        "properties": {
                          "textContent": "{{item.type}}"
                        }
                      },
                      {
                        "library": "@digipair/skill-web-spectrum",
                        "element": "sp-table-cell",
                        "properties": {
                          "textContent": "{{item.message}}"
                        }
                      }
                    ],
                    "conditions": {
                      "each": "EVALUATE:variables.logs"
                    }
                  }
                ]
              }
            ]
          }
        ],
        "properties": {
          "color": "light",
          "scale": "medium"
        }
      },
      {
        "library": "@digipair/skill-web-chatbot",
        "element": "digipair-chatbot",
        "properties": {
          "code": "{{request.digipair}}"
        }
      }
    ],
    "styleHtml": "color-scheme:light;\npadding: 60px 40px 80px 40px;",
    "browserInitialize": [
      {
        "library": "@digipair/skill-web-interact",
        "element": "goTo",
        "properties": {
          "url": "EVALUATE:\"/studio/logs/\" + substring(string(now()), 1, 10)"
        },
        "conditions": {
          "if": "EVALUATE:count(request.params) = 0"
        }
      },
      {
        "library": "@digipair/skill-basic",
        "element": "setVariable",
        "properties": {
          "name": "date",
          "value": "{{request.params.[0]}}"
        }
      },
      {
        "library": "@digipair/skill-basic",
        "element": "setVariable",
        "properties": {
          "name": "logs",
          "execute": [
            {
              "library": "@digipair/skill-web",
              "element": "executeFactory",
              "properties": {
                "execute": [
                  {
                    "library": "@digipair/skill-logger",
                    "element": "read",
                    "properties": {
                      "date": "{{variables.date}}",
                      "type": "factory"
                    }
                  },
                  {
                    "library": "@digipair/skill-basic",
                    "element": "transform",
                    "properties": {
                      "value": "EVALUATE:reverse(previous)"
                    }
                  }
                ]
              }
            }
          ]
        }
      },
      {
        "library": "@digipair/skill-basic",
        "element": "setVariable",
        "properties": {
          "name": "dates",
          "execute": [
            {
              "library": "@digipair/skill-web",
              "element": "executeFactory",
              "properties": {
                "execute": [
                  {
                    "library": "@digipair/skill-logger",
                    "element": "list",
                    "properties": {
                      "type": "factory"
                    }
                  }
                ]
              }
            }
          ]
        }
      }
    ]
  },
  "pins": []
}