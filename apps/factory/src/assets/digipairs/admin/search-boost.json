{
  "name": "Recherche des boosts",
  "description": "Recherche des boosts",
  "library": "@digipair/skill-web",
  "element": "page",
  "metadata": {},
  "properties": {
    "body": [
      {
        "library": "@digipair/skill-web-spectrum",
        "element": "sp-theme",
        "pins": [
          {
            "library": "web",
            "element": "section",
            "pins": [
              {
                "library": "web",
                "element": "section",
                "pins": [
                  {
                    "library": "@digipair/skill-web-spectrum",
                    "element": "sp-table",
                    "pins": [
                      {
                        "library": "@digipair/skill-web-spectrum",
                        "element": "sp-table-head",
                        "pins": [
                          {
                            "library": "@digipair/skill-web-spectrum",
                            "element": "sp-table-head-cell",
                            "properties": {
                              "textContent": "Boost"
                            }
                          },
                          {
                            "library": "@digipair/skill-web-spectrum",
                            "element": "sp-table-head-cell",
                            "properties": {
                              "textContent": "bm25sum"
                            }
                          },
                          {
                            "library": "@digipair/skill-web-spectrum",
                            "element": "sp-table-head-cell",
                            "properties": {
                              "textContent": "semantic_score"
                            }
                          }
                        ]
                      },
                      {
                        "library": "@digipair/skill-web-spectrum",
                        "element": "sp-table-body",
                        "pins": [
                          {
                            "library": "@digipair/skill-web-spectrum",
                            "element": "sp-table-row",
                            "pins": [
                              {
                                "library": "@digipair/skill-web-spectrum",
                                "element": "sp-table-cell",
                                "properties": {
                                  "textContent": "{{item.content}}"
                                }
                              },
                              {
                                "library": "@digipair/skill-web-spectrum",
                                "element": "sp-table-cell",
                                "properties": {
                                  "textContent": "{{item.matchfeatures.bm25sum}}"
                                }
                              },
                              {
                                "library": "@digipair/skill-web-spectrum",
                                "element": "sp-table-cell",
                                "properties": {
                                  "textContent": "{{item.matchfeatures.semantic_score}}"
                                }
                              }
                            ],
                            "conditions": {
                              "each": "EVALUATE:variables.boosts"
                            }
                          }
                        ]
                      }
                    ]
                  }
                ],
                "properties": {
                  "style": "width: 100%; max-width: 1080px;"
                }
              }
            ],
            "properties": {
              "style": "display: flex; justify-content: center;"
            }
          }
        ],
        "properties": {
          "color": "dark",
          "scale": "large"
        }
      }
    ],
    "factoryInitialize": [
      {
        "library": "@digipair/skill-data-management",
        "element": "setVariable",
        "properties": {
          "name": "boosts",
          "execute": [
            {
              "library": "@digipair/skill-vespa",
              "element": "search",
              "properties": {
                "query": "{{request.params.[1]}}",
                "collection": "boost",
                "filter": "source matches \"{{request.params.[0]}}\""
              }
            }
          ]
        }
      }
    ],
    "styleHtml": "color-scheme:dark;"
  },
  "pins": []
}
