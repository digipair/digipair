{
  "library": "@digipair/skill-web",
  "element": "page",
  "metadata": {},
  "properties": {
    "body": [
      {
        "library": "@digipair/skill-web-editor",
        "element": "digipair-editor",
        "properties": {
          "digipair": "EVALUATE:data.digipair",
          "reasoning": "EVALUATE:data.reasoning"
        },
        "events": {
          "save": [
            {
              "library": "@digipair/skill-web-factory",
              "element": "post",
              "properties": {
                "digipair": "{{request.digipair}}",
                "reasoning": "service-reasoning-save",
                "body": "EVALUATE:event"
              }
            },
            {
              "library": "@digipair/skill-web-editor",
              "element": "information",
              "properties": { "message": "Successfully saved !" }
            }
          ]
        }
      }
    ],
    "data": [
      {
        "name": "digipair",
        "value": [
          {
            "library": "@digipair/skill-editor",
            "element": "digipair",
            "properties": {
              "digipair": "{{request.params.[0]}}"
            }
          }
        ]
      },
      {
        "name": "reasoning",
        "value": [
          {
            "library": "@digipair/skill-editor",
            "element": "reasoning",
            "properties": {
              "digipair": "{{request.params.[0]}}",
              "reasoning": "{{request.params.[1]}}"
            }
          }
        ]
      }
    ]
  },
  "pins": []
}
