{
  "library": "@digipair/skill-web",
  "element": "page",
  "metadata": {},
  "properties": {
    "body": [
      {
        "library": "@digipair/skill-web-spectrum",
        "element": "sp-theme",
        "properties": {
          "scale": "large",
          "theme": "dark"
        },
        "pins": [
          {
            "library": "@digipair/skill-web-spectrum",
            "element": "sp-table",
            "pins": [
              {
                "library": "@digipair/skill-web-spectrum",
                "element": "sp-table-head",
                "pins": [
                  {
                    "library": "@digipair/skill-web-spectrum",
                    "element": "sp-table-head-cell",
                    "properties": {
                      "textContent": "Raisonnements"
                    }
                  }
                ]
              },
              {
                "library": "@digipair/skill-web-spectrum",
                "element": "sp-table-body",
                "pins": [
                  {
                    "library": "@digipair/skill-web-spectrum",
                    "element": "sp-table-row",
                    "pins": [
                      {
                        "library": "@digipair/skill-web-spectrum",
                        "element": "sp-table-cell",
                        "properties": {
                          "textContent": "{{item}}"
                        }
                      }
                    ],
                    "conditions": {
                      "each": "EVALUATE:data.reasonings"
                    }
                  }
                ]
              }
            ],
            "events": {
              "change": [
                {
                  "library": "@digipair/skill-web-debug",
                  "element": "console",
                  "properties": {
                    "label": "reasoning",
                    "value": "EVALUATE:event"
                  }
                }
              ]
            }
          }
        ]
      }
    ],
    "data": [
      {
        "name": "reasonings",
        "value": [
          {
            "library": "@digipair/skill-editor",
            "element": "reasonings",
            "properties": {
              "digipair": "{{request.params.[0]}}"
            }
          }
        ]
      }
    ]
  },
  "pins": []
}
