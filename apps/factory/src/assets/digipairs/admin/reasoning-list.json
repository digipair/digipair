{
  "name": "Liste des raisonnements",
  "description": "Liste des raisonnements",
  "library": "@digipair/skill-web",
  "element": "page",
  "metadata": {},
  "properties": {
    "body": [
      {
        "library": "@digipair/skill-web-spectrum",
        "element": "sp-theme",
        "pins": [
          {
            "library": "web",
            "element": "section",
            "pins": [
              {
                "library": "web",
                "element": "section",
                "pins": [
                  {
                    "library": "@digipair/skill-web-spectrum",
                    "element": "sp-action-group",
                    "pins": [
                      {
                        "library": "@digipair/skill-web-spectrum",
                        "element": "sp-action-button",
                        "properties": {
                          "textContent": "Liste des digipairs"
                        },
                        "events": {
                          "click": [
                            {
                              "library": "@digipair/skill-web-interact",
                              "element": "goTo",
                              "properties": {
                                "url": "/admin/digipair-list"
                              }
                            }
                          ]
                        }
                      }
                    ]
                  },
                  {
                    "library": "@digipair/skill-web-spectrum",
                    "element": "sp-divider",
                    "properties": {
                      "style": "margin-top: 1rem; margin-bottom: 2rem;"
                    }
                  },
                  {
                    "library": "@digipair/skill-web-spectrum",
                    "element": "sp-table",
                    "pins": [
                      {
                        "library": "@digipair/skill-web-spectrum",
                        "element": "sp-table-head",
                        "pins": [
                          {
                            "library": "@digipair/skill-web-spectrum",
                            "element": "sp-table-head-cell",
                            "properties": {
                              "textContent": "Raisonnement"
                            }
                          },
                          {
                            "library": "@digipair/skill-web-spectrum",
                            "element": "sp-table-head-cell",
                            "properties": {
                              "textContent": "Description"
                            }
                          },
                          {
                            "library": "@digipair/skill-web-spectrum",
                            "element": "sp-table-head-cell",
                            "properties": {
                              "textContent": ""
                            }
                          }
                        ]
                      },
                      {
                        "library": "@digipair/skill-web-spectrum",
                        "element": "sp-table-body",
                        "pins": [
                          {
                            "library": "@digipair/skill-web-spectrum",
                            "element": "sp-table-row",
                            "pins": [
                              {
                                "library": "@digipair/skill-web-spectrum",
                                "element": "sp-table-cell",
                                "properties": {
                                  "textContent": "{{item.name}}"
                                }
                              },
                              {
                                "library": "@digipair/skill-web-spectrum",
                                "element": "sp-table-cell",
                                "properties": {
                                  "textContent": "{{item.description}}"
                                }
                              },
                              {
                                "library": "@digipair/skill-web-spectrum",
                                "element": "sp-table-cell",
                                "pins": [
                                  {
                                    "library": "@digipair/skill-web-spectrum",
                                    "element": "sp-action-group",
                                    "pins": [
                                      {
                                        "library": "@digipair/skill-web-spectrum",
                                        "element": "sp-action-button",
                                        "pins": [
                                          {
                                            "library": "@digipair/skill-web-spectrum",
                                            "element": "sp-icon",
                                            "properties": {
                                              "src": "data:image/svg+xml;base64,",
                                              "slot": "icon"
                                            }
                                          }
                                        ],
                                        "events": {
                                          "click": [
                                            {
                                              "library": "@digipair/skill-web-interact",
                                              "element": "goTo",
                                              "properties": {
                                                "url": "/admin/reasoning-editor/{{request.params.[0]}}/{{item.id}}"
                                              }
                                            }
                                          ]
                                        }
                                      }
                                    ],
                                    "properties": {
                                      "size": "xs",
                                      "dir": "rtl"
                                    }
                                  }
                                ]
                              }
                            ],
                            "conditions": {
                              "each": "EVALUATE:data.reasonings"
                            }
                          }
                        ]
                      }
                    ]
                  }
                ],
                "properties": {
                  "style": "width: 100%; max-width: 1080px;"
                }
              }
            ],
            "properties": {
              "style": "display: flex; justify-content: center;"
            }
          }
        ],
        "properties": {
          "color": "dark",
          "scale": "large"
        }
      }
    ],
    "data": [
      {
        "name": "reasonings",
        "value": [
          {
            "library": "@digipair/skill-editor",
            "element": "reasonings",
            "properties": {
              "digipair": "{{request.params.[0]}}"
            }
          },
          {
            "library": "@digipair/skill-data-management",
            "element": "transform",
            "properties": {
              "execute": [
                {
                  "library": "@digipair/skill-editor",
                  "element": "reasoning",
                  "properties": {
                    "digipair": "{{request.params.[0]}}",
                    "reasoning": "EVALUATE:item"
                  }
                }
              ]
            },
            "conditions": {
              "each": "EVALUATE:previous"
            }
          }
        ]
      }
    ],
    "styleHtml": "color-scheme:dark;"
  },
  "pins": []
}
