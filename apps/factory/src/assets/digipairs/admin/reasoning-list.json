                                              "element": "confirm",
                                              "properties": {
                                                "message": "Are you sure to remove this schedule ?",
                                                "ok": [
                                                  {
                                                    "library": "@digipair/skill-web",
                                                    "element": "executeFactory",
                                                    "properties": {
                                                      "execute": [
                                                        {
                                                          "library": "@digipair/skill-cron",
                                                          "element": "deleteCron",
