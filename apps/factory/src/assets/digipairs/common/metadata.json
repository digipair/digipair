{
  "name": "Récupération des metadatas",
  "description": "Récupère les metadatas d'un digipair",
  "metadata": {},
  "library": "@digipair/skill-service",
  "element": "service",
  "properties": {
    "execute": [
      {
        "library": "@digipair/skill-common",
        "element": "metadata",
        "properties": {
          "digipair": "{{request.body.digipair}}"
        }
      }
    ]
  }
}
